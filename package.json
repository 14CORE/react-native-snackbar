{
  "name": "react-native-snackbar",
  "version": "0.3.8",
  "description": "Material-design \"Snackbar\" component for Android and iOS",
  "main": "lib/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "RNSnackbar.podspec"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src example --ext .js,.jsx",
    "flow": "flow src",
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src --out-dir lib --ignore src/__tests__",
    "prepublish": "npm run build"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cooperka/react-native-snackbar.git"
  },
  "keywords": [
    "react-native",
    "component",
    "ios",
    "android",
    "material design",
    "snackbar",
    "snack bar"
  ],
  "author": "Azendoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cooperka/react-native-snackbar/issues"
  },
  "homepage": "https://github.com/cooperka/react-native-snackbar",
  "peerDependencies": {
    "react": ">=15.1 || >=16.0.0-alpha.6",
    "react-native": ">=0.40"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-cooperka": "0.2.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-react-native": "2.3.2",
    "flow-bin": "0.40.0",
    "jest": "20.0.4",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1"
  }
}
